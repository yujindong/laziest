{
  "name": "@laziest/hooks",
  "private": false,
  "sideEffects": false,
  "version": "0.0.0",
  "description": "React Hooks",
  "keywords": [
    "react hooks",
    "hooks",
    "react",
    "laziest"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --max-warnings 0"
  },
  "author": "yujindong <yujindong1985@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@laziest/eslint-config": "workspace:^",
    "@laziest/typescript-config": "workspace:^",
    "@types/react": "19.1.0",
    "eslint": "^9.28.0",
    "react": "^19.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19"
  },
  "dependencies": {
    "@laziest/utils": "workspace:*",
    "resize-observer-polyfill": "^1.5.1"
  }
}
